<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" fx:id="rootPane" prefHeight="434.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="moviebookingsystem.add.movies.AddMovieController">
    <stylesheets>
        <URL value="@addmovie.css" />
    </stylesheets>
   <children>
      <VBox layoutX="10.0" layoutY="10.0" prefHeight="413.0" prefWidth="610.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <JFXTextField fx:id="movie_name" labelFloat="true" promptText="Movie Name">
               <VBox.margin>
                  <Insets bottom="30.0" left="50.0" right="50.0" top="20.0" />
               </VBox.margin>
               <font>
                  <Font size="13.0" />
               </font>
            </JFXTextField>
            <JFXComboBox fx:id="company_combo" prefHeight="25.0" prefWidth="553.0" promptText="~~ Choose Company ~~">
               <VBox.margin>
                  <Insets bottom="30.0" left="50.0" right="50.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXComboBox fx:id="category_combo" layoutX="10.0" layoutY="201.0" prefHeight="25.0" prefWidth="551.0" promptText="~~ Choose Category ~~">
               <VBox.margin>
                  <Insets bottom="30.0" left="50.0" right="50.0" />
               </VBox.margin>
            </JFXComboBox>
            <JFXTextField fx:id="cinema_hall" labelFloat="true" layoutX="60.0" layoutY="30.0" promptText="Cinema Hall">
               <font>
                  <Font size="13.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" left="50.0" right="50.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="qty" labelFloat="true" layoutX="60.0" layoutY="87.0" promptText="Price">
               <font>
                  <Font size="13.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="30.0" left="50.0" right="50.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField fx:id="mv_description" labelFloat="true" layoutX="60.0" layoutY="254.0" promptText="Descriptions">
               <font>
                  <Font size="13.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="50.0" left="50.0" right="50.0" />
               </VBox.margin>
            </JFXTextField>
            <HBox prefHeight="53.0" prefWidth="576.0">
               <children>
                  <JFXButton fx:id="saveButton" onAction="#save" prefHeight="35.0" prefWidth="173.0" stylesheets="@addmovie.css" text="SAVE" textFill="WHITE">
                     <HBox.margin>
                        <Insets left="120.0" right="20.0" />
                     </HBox.margin>
                     <styleClass>
                        <String fx:value="jfx-button" />
                        <String fx:value="save-button" />
                     </styleClass>
                  </JFXButton>
                  <JFXButton fx:id="cancelButton" layoutX="60.0" layoutY="10.0" onAction="#cancel" prefHeight="37.0" prefWidth="156.0" stylesheets="@addmovie.css" text="CANCEL" textFill="WHITE">
                     <styleClass>
                        <String fx:value="jfx-button" />
                        <String fx:value="cancel-button" />
                     </styleClass>
                  </JFXButton>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
